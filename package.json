{
  "name": "stompilot",
  "private": true,
  "version": "1.0.0",
  "main": "dist-electron/main.js",
  "type": "module",
  "author": "jbj338033",
  "scripts": {
    "dev": "vite",
    "build": "rimraf dist dist-electron && tsc -p tsconfig.electron.json && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "electron:dev": "rimraf dist dist-electron && tsc -p tsconfig.electron.json && vite dev",
    "electron:build": "rimraf dist dist-electron release && npm run build && electron-builder",
    "electron:preview": "npm run build && electron ."
  },
  "dependencies": {
    "@stomp/stompjs": "^7.0.0",
    "electron-devtools-installer": "^3.2.0",
    "electron-store": "^10.0.0",
    "electron-vite": "^2.3.0",
    "framer-motion": "^11.11.17",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.4.1",
    "react-i18next": "^15.1.1",
    "react-icons": "^5.3.0",
    "react-router-dom": "^6.28.0",
    "zustand": "^5.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.0",
    "electron": "^33.2.0",
    "electron-builder": "^25.1.8",
    "eslint": "^9.13.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.11.0",
    "postcss": "^8.4.49",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.4.15",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.11.0",
    "vite": "^5.4.10",
    "vite-plugin-electron": "^0.29.0",
    "vite-plugin-electron-renderer": "^0.14.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jbj338033/stompilot-app"
  },
  "license": "MIT",
  "description": "Stompilot",
  "keywords": [
    "stomp",
    "stompjs",
    "electron",
    "react",
    "vite",
    "tailwindcss"
  ],
  "build": {
    "productName": "Stompilot",
    "appId": "com.stompilot",
    "asar": true,
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "dist-electron/**/*"
    ],
    "mac": {
      "artifactName": "${productName}_${version}.${ext}",
      "target": [
        "dmg"
      ],
      "icon": "build/icon.png"
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ],
      "artifactName": "${productName}_${version}.${ext}",
      "icon": "build/icon.png"
    }
  }
}
